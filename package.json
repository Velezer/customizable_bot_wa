{
   "name": "customizable_bot_wa",
   "version": "1.0.0",
   "description": "",
   "main": "main.js",
   "scripts": {
      "prepush": "npm run typecheck && npm run test",
      "typecheck": "tsc --noEmit",
      "typecheck:watch": "npm run typecheck -- -w",
      "start": "ts-node src/main.ts",
      "clear-jest": "jest --clearCache",
      "test": "jest",
      "test:coverage": "jest --coverage",
      "tdd": "jest --watch",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migrate:up": "npm run typeorm -- migration:run -d ./src/typeorm/data-source.ts",
      "migration:gen": "npm run typeorm -- migration:generate -d ./src/typeorm/data-source.ts"
   },
   "author": "Velezer",
   "license": "ISC",
   "devDependencies": {
      "@types/better-sqlite3": "^7.5.0",
      "@types/jest": "^27.4.1",
      "@types/node": "^17.0.24",
      "better-sqlite3": "^7.5.1",
      "jest": "^27.5.1",
      "ts-jest": "^27.1.4",
      "typescript": "^4.6.3"
   },
   "dependencies": {
      "@adiwajshing/baileys": "^4.1.0",
      "@types/fluent-ffmpeg": "^2.1.20",
      "@vitalets/google-translate-api": "^8.0.0",
      "axios": "^0.26.1",
      "class-transformer": "^0.5.1",
      "dotenv": "^16.0.0",
      "fluent-ffmpeg": "^2.1.2",
      "jimp": "^0.16.1",
      "kvfiledb": "^0.1.2",
      "pg": "^8.4.0",
      "pg-connection-string": "^2.5.0",
      "reflect-metadata": "^0.1.13",
      "ts-node": "^10.7.0",
      "typeorm": "^0.3.6",
      "ytdl-core": "^4.11.0"
   }
}
